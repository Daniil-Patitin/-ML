      "source": [
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "vectorizer = TfidfVectorizer(min_df=5)\n",
        "X_train_data = vectorizer.fit_transform(salary_train_data['FullDescription'])\n",
        "X_test_data = vectorizer.transform(salary_test_data['FullDescription'])"
      ],
      "execution_count": 24,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "C3i0wMoTW8Ip",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "from sklearn.feature_extraction import DictVectorizer\n",
        "dictVect = DictVectorizer()\n",
        "X_train_category = dictVect.fit_transform(salary_train_data[['LocationNormalized', 'ContractTime']].to_dict('records'))\n",
        "X_test_category = dictVect.transform(salary_test_data[['LocationNormalized', 'ContractTime']].to_dict('records'))\n"
      ],
      "execution_count": 28,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "86yg6nUYZ8dM",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "from scipy.sparse import hstack\n",
        "X_train = hstack([X_train_data, X_train_category])\n",
        "X_test = hstack([X_test_data, X_test_category])\n",
        "\n",
        "\n",
        "Y_train = salary_train_data['SalaryNormalized']"
      ],
      "execution_count": 29,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "39KAog_vaenC",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 34
        },
        "outputId": "5ae82cad-cbb1-4131-b014-e81af2d5bfd2"
      },
      "source": [
        "from sklearn.linear_model import Ridge\n",
        "MODEL = Ridge(alpha=1)\n",
        "MODEL.fit(X_train, Y_train)\n",
        "\n",
        "RESULT = MODEL.predict(X_test)\n",
        "RESULT.round(2)\n",
       
      ],
